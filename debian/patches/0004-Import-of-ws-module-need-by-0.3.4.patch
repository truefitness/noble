+template<typename T> NAN_INLINE bool NanSetPointerSafe(
+    T *var
+  , T val
+) {
+  if (var) {
+    *var = val;
+    return true;
+  } else {
+    return false;
+  }
+}
+
+template<typename T> NAN_INLINE T NanGetPointerSafe(
+    T *var
+  , T fallback = reinterpret_cast<T>(0)
+) {
+  if (var) {
+    return *var;
+  } else {
+    return fallback;
+  }
+}
