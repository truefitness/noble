+  void Call(int argc, v8::Handle<v8::Value> argv[]) {
+    NanScope();
+#if (NODE_MODULE_VERSION > 0x000B)  // 0.11.12+
+    v8::Local<v8::Function> callback = NanNew(handle)->
+        Get(NanSymbol("callback")).As<v8::Function>();
